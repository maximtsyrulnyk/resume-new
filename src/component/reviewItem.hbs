<div class="card">
    <div class="card-body">
        {{#each reviewList}}
        <div class="card">
            <div class="card-body">
                {{#with title}}
                <p><b>{{userName}}</b></p>
                <br>
                <p><b>{{caption}}</b></p>
                <br>
                <p>{{date}}</p>
                {{/with}}
            </div>

        </div>

        <div class="card">
            <div class="card-body">
                {{#with reviewBody}}
                {{#with seller}}
                <p><b>{{title}}</b>
                    <text>{{value}}</text>
                </p>
                {{/with}}
                <br>
                {{#with rating}}
                <p><b>{{title}}</b>
                    <text>{{value}}</text>
                </p>
                {{/with}}
                <br>
                <p><b>{{title}}</b>
                    <text>{{value}}</text>
                </p>
                {{#with advantages}}
                <p><b>{{title}}</b>
                    <text>{{text}}</text>
                </p>
                {{/with}}
                <br>
                {{#with disadvantages}}
                <p><b>{{title}}</b>
                    <text>{{text}}</text>
                </p>
                {{/with}}
                <div class="row">
                    {{#each images}}
                    <img src="{{this}}" alt="">
                    <p></p>
                    {{/each}}
                </div>

                {{#each coments}}
                <div class="card">
                    <div class="card-body">
                        <p><b>{{userName}}</b></p>
                        <p>{{date}}</p>
                        <text>{{text}}</text>
                    </div>
                </div>
                {{/each}}

                {{/with}}
            </div>

        </div>
        <p></p>
        <br>
        {{/each}}


    </div>

</div>